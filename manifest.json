{
  "name": "Oliv Speed Up!",
  "version": "1.2",
  "description": "Speed up an slowly speaker!",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "declarativeContent"],
  "content_scripts": [{
    "js": ["tag-manager.js", "user-insight.js"],
    "matches": ["http://*/*", "https://*/*"]
  }],
  "content_security_policy": "script-src 'self' https://www.googletagmanager.com; object-src 'self'",
  "manifest_version": 2
}
